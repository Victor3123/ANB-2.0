{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": false,
    "baseUrl": "./",
    "paths": {
      "crypto": [
        "node_modules/crypto-js"
      ]
    },
    "esModuleInterop": true,
    "strict": true,
    "sourceMap": true,
    "lib": [
      "es2021"
    ],
  }
}